{
  "from": "${from}",
  "size": "${size}",
  "track_total_hits": true,
  "query": {
    "bool": {
      "must": [
        {
          "match_all": {}
        },
        {
          "range": {
            "end_time": {
              "gte": "${end_time}"
            }
          }
        }
      ]
    }
  },
  "sort": {
    "end_time": {
      "order": "desc"
    }
  }
}